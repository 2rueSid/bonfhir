/**
 * FHIR Definitions for {{ fhir.release }}/{{ fhir.version }}
 */

{% for structureDef in fhir.structureDefinitions %}

  {% if (structureDef.derivation != "constraint") and (structureDef.kind != "primitive-type") %}
    {{ structureDef.jsDoc }}
    export interface {{ structureDef.type }} {% if structureDef.base.type %}extends {{ structureDef.base.type }} {% endif %}{
      {% if structureDef.isDomainResource %}
      readonly resourceType: "{{ structureDef.type }}";
      {% else %}
      readonly resourceType: string;
      {% endif %}
    }

  {% endif %}
{% endfor %}
